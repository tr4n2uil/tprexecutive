/* twitter styles */
.twitter { margin: 5px 10px 5px 5px; padding: 5px; font-size: 11px; font-weight: normal; border: 1px solid green; }
.twitter  .tw-logo { background-color: green; -moz-border-radius: 5px 5px 5px 5px; -webkit-border-radius: 5px 5px 5px 5px; }
#tw-root { max-width: 175px; }
#tw-root a { display: inline !important; padding: 0 !important; }
