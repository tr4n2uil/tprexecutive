body {	width: 100%;	margin: 0;	padding: 0;}#load-status {	position: fixed;	top: 0;	left: 40%;	width: 20%;	text-align: center;	background: yellow;	font-size: 12px;}div#firespark_console {	padding: 0.5em 1.5em;	margin: auto;	background: url("../img/executive/background-gold.jpg");}div#top-panel {	margin: 1em 0;}div#centre-panel {	margin: 1em 0;}div#bottom-panel {	margin: 1em 0;}div#header {	float: left;	text-align: left;}div#header p {	font-size: large;	padding: 2px;}div#header .large {	padding-top: 15px;	font-weight: bold;	font-size: xx-large;}div#menu-container {	text-align: center;	float: right;}div#quick-panel {	width: 15%;	float: left;	text-align: left;}div#main-container {	float: left;	width: 85%;	margin: auto;}div#footer {	text-align: center;}div#validation-panel {	float: right;}